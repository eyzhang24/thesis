	file = {Stevens et al. - 2022 - Urinary phthalate metabolite mixtures in pregnancy.pdf:/Users/elizabethzhang/Zotero/storage/2VHTI6VY/Stevens et al. - 2022 - Urinary phthalate metabolite mixtures in pregnancy.pdf:application/pdf},
