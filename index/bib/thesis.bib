	file = {Halford et al. - 2005 - How Many Variables Can Humans Process.pdf:/Users/elizabethzhang/Zotero/storage/889B2EUF/Halford et al. - 2005 - How Many Variables Can Humans Process.pdf:application/pdf},
}

